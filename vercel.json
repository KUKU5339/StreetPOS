{
    "version": 2,
    "framework": null,
    "outputDirectory": "public",
    "buildCommand": "npm run build",
    "builds": [
        { "src": "api/index.php", "use": "vercel-php@0.7.4" },
        { "src": "public/**", "use": "@vercel/static" }
    ],
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        {
            "src": "/offline-db\\.js",
            "dest": "/public/offline-db.js"
        },
        {
            "src": "/manifest\\.json",
            "dest": "/public/manifest.json"
        },
        {
            "src": "/sw\\.js",
            "dest": "/public/sw.js"
        },
        {
            "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))$",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://pos-web-khaki.vercel.app",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "DB_CONNECTION": "pgsql",
        "DB_URL": "",
        "SUPABASE_URL": "https://jbxuzfbpgcsosuiehmsz.supabase.co",
        "SUPABASE_STORAGE_BUCKET": "products",
        "VITE_SUPABASE_URL": "https://jbxuzfbpgcsosuiehmsz.supabase.co",
        "VITE_SUPABASE_ANON_KEY": "sb_publishable_p1Y_vlB0ICibE-5asof7Gw_cHt0kQiE"
    }
}
